<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Navigator.Dopolnenie.enetry"
             Title="entry">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="entery"
                Placeholder="Username"
                PlaceholderColor="Olive"
                HeightRequest="100"
                WidthRequest="200"/>
            <Entry x:Name="entery2"
                   IsPassword="true" 
                   Placeholder="Password"></Entry>
            <Entry x:Name="entery3"
                   Placeholder="ID"
                   MaxLength="11"
                   Keyboard="Numeric"></Entry>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>